<script> 
new Swiper('.swiper-container', {
  loop: true,
  autoplay: 3000,
  pagination: '.swiper-pagination',
  paginationClickable: true,
});

var typeds = [];
function setupTyped() {
  if (typeds.length > 0) {
    typeds.forEach(function(typed) {
      typed.destroy();
    });
    typeds = [];
  }
  var lines = $('#code .line');
  lines.each(function(index, element) {
    var _this = $(element);
    _this.html('');
    var isResult = _this.hasClass('result') ? true : false;
    setTimeout(function() {
      if (isResult) {
        _this.html(_this.attr('text'));
      } else {
        var typed = new Typed(element, {
          strings: [ _this.attr('text') ],
          typeSpeed: 80,
          startDelay: 0,
          onComplete: function(pos, self) { 
            $('.typed-cursor').remove();
          },
        });
        typeds.push(typed);
      }
    }, 3000 * index); 
  });
}

setupTyped();
setInterval(setupTyped, 25000);

$('#index .plans .slider .items .item').hover(function() {
  $('#index .plans .slider .items .item').removeClass('active');
  $(this).addClass('active');
  var text = $(this).attr('text');
  var date = $(this).children(".type").text();

  $('#index .plans .detail .type').html(date);
  $('#index .plans .detail .target').html(text);
});

function lightboxOpen() {
  $('#video-box').show();
  $('body').addClass('stop-scrolling');
  var lightBoxVideo = $('#IntroVideo')[0];
  window.scrollTo(0, 0);
  lightBoxVideo.play();
}

function lightboxClose() {
  $('#video-box').hide();
  $('body').removeClass('stop-scrolling');
  var lightBoxVideo = $('#IntroVideo')[0];
  lightBoxVideo.pause();
  window.scrollTo(0, 4200)
};


/**星空效果 */
var runCanvas = function() {
    var canvasEl = document.getElementById("canvas")
    var ctx = canvasEl.getContext("2d")
    var mousePos = [0, 0]
    var nodes = [] //星星
    var edges = [] //连线

    var easingFactor = 5.0 //缓动因子
    var backgroundColor = "#000" //背景颜色
    // var backgroundColor = "rgba(255, 255, 255, 0)" //背景颜色
    var nodeColor = "#fff" //点颜色
    var edgeColor = "#fff" //边颜色

    var pageWidth = $("#header").width(), //窗口宽度
        pageHeight = $("#header").innerHeight() //窗口高度

    window.onresize = function() {
        ;(pageWidth = $("#header").width()),
            (pageHeight = $("#header").innerHeight())
        canvasEl.width = pageWidth
        canvasEl.height = pageHeight

        if (nodes.length == 0) {
            constructNodes()
        }

        render()
    }

    window.onresize()

    /**构建星星 */
    function constructNodes() {
        for (var i = 0; i < 100; i++) {
            var node = {
                drivenByMouse: i == 0,
                x: Math.random() * canvasEl.width,
                y: Math.random() * canvasEl.height,
                vx: Math.random() * 1 - 0.5,
                vy: Math.random() * 1 - 0.5,
                radius:
                    Math.random() > 0.9
                        ? 3 + Math.random() * 3
                        : 1 + Math.random() * 3
            }

            nodes.push(node)
        }

        nodes.forEach(function(e) {
            nodes.forEach(function(e2) {
                if (e == e2) {
                    return
                }

                var edge = {
                    from: e,
                    to: e2
                }

                addEdge(edge)
            })
        })
    }
    function addEdge(edge) {
        var ignore = false

        edges.forEach(function(e) {
            if ((e.from == edge.from) & (e.to == edge.to)) {
                ignore = true
            }

            if ((e.to == edge.from) & (e.from == edge.to)) {
                ignore = true
            }
        })

        if (!ignore) {
            edges.push(edge)
        }
    }

    function step() {
        nodes.forEach(function(e) {
            if (e.drivenByMouse) {
                return
            }

            e.x += e.vx
            e.y += e.vy

            function clamp(min, max, value) {
                if (value > max) {
                    return max
                } else if (value < min) {
                    return min
                } else {
                    return value
                }
            }

            if (e.x <= 0 || e.x >= canvasEl.width) {
                e.vx *= -1
                e.x = clamp(0, canvasEl.width, e.x)
            }

            if (e.y <= 0 || e.y >= canvasEl.height) {
                e.vy *= -1
                e.y = clamp(0, canvasEl.height, e.y)
            }
        })

        adjustNodeDrivenByMouse()
        render()
        window.requestAnimationFrame(step)
    }

    function adjustNodeDrivenByMouse() {
        nodes[0].x += (mousePos[0] - nodes[0].x) / easingFactor
        nodes[0].y += (mousePos[1] - nodes[0].y) / easingFactor
    }

    function render() {
        ctx.fillStyle = backgroundColor
        // ctx.fillRect(0, 0, canvasEl.width, canvasEl.height)
        ctx.clearRect(0, 0, canvasEl.width, canvasEl.height)

        edges.forEach(function(e) {
            var l = lengthOfEdge(e)
            var threshold = canvasEl.width / 8

            if (l > threshold) {
                return
            }

            ctx.strokeStyle = edgeColor
            ctx.lineWidth = (1.0 - l / threshold) * 2.5
            ctx.globalAlpha = 1.0 - l / threshold
            ctx.beginPath()
            ctx.moveTo(e.from.x, e.from.y)
            ctx.lineTo(e.to.x, e.to.y)
            ctx.stroke()
        })
        ctx.globalAlpha = 1.0

        nodes.forEach(function(e) {
            if (e.drivenByMouse) {
                return
            }

            ctx.fillStyle = nodeColor
            ctx.beginPath()
            ctx.arc(e.x, e.y, e.radius, 0, 2 * Math.PI)
            ctx.fill()
        })
    }
    function lengthOfEdge(edge) {
        return Math.sqrt(
            Math.pow(edge.from.x - edge.to.x, 2) +
                Math.pow(edge.from.y - edge.to.y, 2)
        )
    }

    window.onmousemove = function(e) {
        if (e.pageX > pageWidth || e.pageY > pageHeight) return
        // console.log(e.pageX, e.pageY)
        mousePos[0] = e.pageX
        mousePos[1] = e.pageY
    }

    window.requestAnimationFrame(step)
}
runCanvas()
</script>

