@keyframes LokiBlur {
    from { filter: blur(8px); }
    50%  { filter: blur(2px); }
    to   { filter: blur(8px); }
 }

@keyframes LokiSliderLogoPulseFrames {
    from { opacity: 0; }
    40% { opacity: 0; }
    60% { opacity: 1; }
    90% { opacity: 0; }
    to { opacity: 0; }
}

.LokiSlider {
    background-image:  linear-gradient(to bottom, rgba(255, 255, 255, 0), rgba(255, 255, 255, 0) 80%, #1a1a1a 100%),
                                         linear-gradient(to right,      #1a1a1a 0%,                 rgba(255, 255, 255, 0) 60%),
                                         url("https://loki.network/wp-content/uploads/2018/03/LokiIllustrationOptimised16.svg");

    background-repeat:   no-repeat;
    background-position: center center;
    background-size:     cover;
    width:               100%;
    height:              100%;
    left:                0;
    top:                 0;
    position:            absolute;
}

.LokiSliderLogoPulsing {
    animation:           LokiSliderLogoPulseFrames 4s linear 0s infinite;
    background-image:    url("https://loki.network/wp-content/uploads/2018/03/LokiIllustrationLogo.svg");
    background-repeat:   no-repeat;
    background-position: center center;
    background-size:     cover;
    width:               100%;
    height:              100%;
    left:                0;
    position:            absolute;
    transform:           translate3d(0, 0, 0); /* PERF Hack */
}

.LokiSliderHighlights {
    animation:           LokiBlur 4s ease-in-out 0s infinite;
    background-image:    url("https://loki.network/wp-content/uploads/2018/03/Loki-IllustrationOptimised12-JustHighlights.svg");
    background-repeat:   no-repeat;
    background-position: center center;
    background-size:     cover;
    width:               100%;
    height:              100%;
    left:                0;
    position:            absolute;
}

@media screen and (max-width: 800px) {
    .LokiSliderHighlights {
        display: none;
    }
}


.edgtf-page-footer {
    display: inline;
}

